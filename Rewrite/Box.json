	{
      "id": "gasolinePrice",
      "name": "今日油价",
      "keys": [
        "@gasolinePrice.location",
        "@gasolinePrice.type",
        "@gasolinePrice.isNotify",
        "@gasolinePrice.isShowAll"
      ],
